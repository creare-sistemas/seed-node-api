{
  "name": "seed-nodejs",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "migration": "knex migrate:latest",
    "migration:rollback": "knex-migrate rollback",
    "start": "npm run migration && node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "test:unit": "mocha --opts test/unit/mocha.opts 'test/unit/**/*.js'",
    "test:integration": "mocha --opts test/integration/mocha.opts 'test/integration/**/*.js'",
    "lint": "eslint src test",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "pm2-start": "pm2 run process.yml",
    "pm2-stop": "pm2 stop process.yml",
    "pm2-restart": "pm2 restart process.yml",
    "openapi:swagger2-to-openapi": "./node_modules/.bin/swagger2openapi -r -y docs/swagger.yml -o docs/openapi.yml -p",
    "postman-collection:openapi-to-postman": "./node_modules/.bin/openapi2postmanv2 -s docs/openapi.yml -o docs/imageRecognitionPostmanCollection.json -p",
    "build:openapi-and-postman-collection": " npm run openapi:swagger2-to-openapi && npm run postman-collection:openapi-to-postman"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/creare-sistemas/seed-project-nodejs-creare"
  },
  "author": "Creare Sistemas",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "@hapi/joi-date": "^1.3.0",
    "aws-sdk": "^2.593.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.0",
    "dotenv": "^8.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "helmet": "^3.20.0",
    "http-errors": "^1.7.3",
    "i18n": "^0.8.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.11",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "ms": "^2.1.2",
    "multer": "^1.4.2",
    "openapi-to-postmanv2": "0.0.10",
    "pg": "^7.17.0",
    "prettier": "^1.19.1",
    "request": "^2.88.0",
    "request-debug": "^0.2.0",
    "sqlite3": "^4.1.1",
    "swagger2openapi": "^5.2.5",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.18.2",
    "jsdoc": "^3.6.3",
    "knex-migrate": "^1.7.2",
    "mocha": "^6.2.0",
    "nock": "^10.0.6",
    "nodemon": "2.0.2",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "supertest": "^4.0.2"
  }
}
